{
    "type": "object",
    "description": "Get API default for specific entity (app, stream etc.)",
    "required": [
        "name",
        "operation",
        "details"
    ],
    "properties": {
        "name": {
            "type": "string"
        },
        "operation": {
            "type": "string",
            "enum": [
                "about.apiDescription"
            ]
        },
        "skip": {
            "type": "boolean"
        },
        "details": {
            "type": "object",
            "required": [
                "path"
            ],
            "properties": {
                "path": {
                    "type": "string"
                }
            }
        }
    }
}